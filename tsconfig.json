{
    "compilerOptions": {
      "module": "commonjs",
      "target": "es5",
      "sourceMap": true,
      "outDir": "build",
      "rootDir": "src",
        "types": [
          "node"
        ]
    },
    "include": [
      "./src/**/*.ts",
      "./src/**/*.tsx"
    ],
    "exclude": [
      "node_modules",
      "./src/**/*.spec.ts",
      "./keys/**/*"
    ]
}